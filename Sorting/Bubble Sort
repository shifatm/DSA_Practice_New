TC: O(N^2) - Average and Worst
TC: O(N) - Best when the array is already sorted.
SC: O(1)


void bubbleSort( int arr[], int n){
	for(int i = n-1; i>=1; i++){
	int didSwap = 0;
		for(int j = 0; j  <= i-2; i++){
			if(arr[j] > arr[j+1]){
				swap( arr[j] > arr[j+1]);
				didSwap = 1;
			}
		}
		if(didSwap == 0){
			break;
		}
	}
}

int main(){
	int n;
	cin>>n;
	int array[n];
	for(int i = 0; i<n; i++){	
		cin>>array[i];
}
bubbleSort(array, n);
for(int i = 0; i<n; i++){	
		cout<< array[i];
}
		
}
