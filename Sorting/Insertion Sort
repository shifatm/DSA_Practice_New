TC: O(N^2)- Worst and Average
O(N) - Best
SC: O(1)

void insertionSort(int arr[], int n){
	for(int i = 0; i < n-1; i++){
		int j = i;
		while(j > 0 && arr[j-1] > arr[j]){
			swap(arr[j-1], arr[j]);
			jâ€“;
		}
	}
}

int main(){
	int n;
	cin>>n;
	int array[n];
	for(int i = 0; i<n; i++){	
		cin>>array[i];
}
selectionSort(array, n);
for(int i = 0; i<n; i++){	
		cout<< array[i];
}
		
}
